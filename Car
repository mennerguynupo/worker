#include <iostream>
using namespace std;

class Car {
public:
    string brand;
    int speed;

    void displayInfo() {
        cout << "品牌: " << brand << ", 速度: " << speed << " km/h" << endl;
    }
};

int main() {
    Car car1;
    car1.brand = "Toyota";
    car1.speed = 120;
    car1.displayInfo();

    return 0;
}
